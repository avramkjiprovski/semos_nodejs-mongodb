{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "eol-last": "warn",
        "no-unused-vars": "warn",
        "object-curly-newline": "off"
    }
}